r\">\r\n                <div class=\"row\">\r\n                    <div class=\"footer-col-1\">\r\n                        <h3>Download Our App</h3>\r\n                        <p>Download App for Android and ios mobile phone.</p>\r\n                        <div class=\"app-logo\">\r\n                            <img src=\"images/play-store.png\">\r\n                            <img src=\"images/app-store.png\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"footer-col-2\">\r\n                        <img src=\"images/logo-white.png\">\r\n                        <p>Our Purpose Is To Sustainably Make the Pleasure and Benefits of Sports Accessible to the Many.\r\n                        </p>\r\n                    </div>\r\n                    <div class=\"footer-col-3\">\r\n                        <h3>Useful Links</h3>\r\n                        <ul>\r\n                            <li>Coupons</li>\r\n                            <li>Blog Post</li>\r\n                            <li>Return Policy</li>\r\n                            <li>Join Affiliate</li>\r\n                        </ul>\r\n                    </div>\r\n                    <div class=\"footer-col-4\">\r\n                        <h3>Follow Us</h3>\r\n                        <ul>\r\n                            <li>Facebook</li>\r\n                            <li>Twitter</li>\r\n                            <li>Instagram</li>\r\n                            <li>Youtube</li>\r\n                        </ul>\r\n                    </div>\r\n                </div>\r\n                <hr>\r\n                <p class=\"copyright\">Copyright 2020 - Samwit Adhikary</p>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- javascript -->\r\n        <component :is=\"'script'\">\r\n            var MenuItems = document.getElementById(\"MenuItems\");\r\n            MenuItems.style.maxHeight = \"0px\";\r\n            function menutoggle() {\r\n            if (MenuItems.style.maxHeight == \"0px\") {\r\n            MenuItems.style.maxHeight = \"200px\"\r\n            }\r\n            else {\r\n            MenuItems.style.maxHeight = \"0px\"\r\n            }\r\n            }\r\n        </component>\r\n        <!-- product gallery -->\r\n\r\n        <component :is=\"'script'\">\r\n            var ProductImg = document.getElementById(\"ProductImg\");\r\n            var SmallImg = document.getElementsByClassName(\"small-img\");\r\n\r\n            SmallImg[0].onclick = function () {\r\n            ProductImg.src = SmallImg[0].src;\r\n            }\r\n            SmallImg[1].onclick = function () {\r\n            ProductImg.src = SmallImg[1].src;\r\n            }\r\n            SmallImg[2].onclick = function () {\r\n            ProductImg.src = SmallImg[2].src;\r\n            }\r\n            SmallImg[3].onclick = function () {\r\n            ProductImg.src = SmallImg[3].src;\r\n            }\r\n        </component>\r\n    </body>\r\n\r\n    </html>\r\n</template>\r\n\r\n<script>\r\nimport store from \"../store/store\";\r\nimport MyHeader from \"./molcutes/MyHeader\";\r\n\r\nimport FullCalendar from '@fullcalendar/vue3'\r\nimport dayGridPlugin from '@fullcalendar/daygrid'\r\n//import interactionPlugin from '@fullcalendar/interaction'\r\nimport FullCalendarInteraction from '@fullcalendar/interaction';\r\n\r\nexport default {\r\n    name: 'WakouProductDetails',\r\n    data() {\r\n        document.title = \"和光市のイベントをカレンダーから探す!\"\r\n        return Object.assign(\r\n            {\r\n                store: store,\r\n                store_top4: Object.fromEntries(Object.entries(store).slice(0, 4)),\r\n                calendarOptions: {\r\n                    plugins: [dayGridPlugin, FullCalendarInteraction],\r\n                    headerToolbar: {\r\n                        left: '',\r\n                        center: 'title',\r\n                        right: 'prev,next'\r\n                    },\r\n                    locale: 'ja',\r\n                    initialView: 'dayGridMonth',\r\n                    dateClick: this.handleDateClick,\r\n                    eventClick: this.handleEventClick,\r\n                    events: Object.values(store).map(row => Object.assign({\r\n                        title: row.title,\r\n                        date: row.split_date[0] + \"-\" + String(row.split_date[1]).padStart(2, '0') + \"-\" + row.split_date[2],\r\n                        id : row.id\r\n                    }))\r\n\r\n                }\r\n            })\r\n    },\r\n    components: {\r\n        'MyHeader': MyHeader,\r\n        'FullCalendar': FullCalendar // make the <FullCalendar> tag available\r\n    },\r\n    methods: {\r\n        handleDateClick: function () {\r\n        },\r\n        handleEventClick: function (arg) {\r\n            console.log(arg)\r\n            console.log(arg.event._def.publicId)\r\n            window.location.href = `./product_details.html?id=${ arg.event._def.publicId}`\r\n        }\r\n    }\r\n}\r\n\r\n</script>\r\n"],"mappings":"AA8HA,OAAOA,KAAI,MAAO,gBAAgB;AAClC,OAAOC,QAAO,MAAO,qBAAqB;AAE1C,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,aAAY,MAAO,uBAAsB;AAChD;AACA,OAAOC,uBAAsB,MAAO,2BAA2B;AAE/D,eAAe;EACXC,IAAI,EAAE,qBAAqB;EAC3BC,IAAIA,CAAA,EAAG;IACHC,QAAQ,CAACC,KAAI,GAAI,qBAAoB;IACrC,OAAOC,MAAM,CAACC,MAAM,CAChB;MACIV,KAAK,EAAEA,KAAK;MACZW,UAAU,EAAEF,MAAM,CAACG,WAAW,CAACH,MAAM,CAACI,OAAO,CAACb,KAAK,CAAC,CAACc,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;MACjEC,eAAe,EAAE;QACbC,OAAO,EAAE,CAACb,aAAa,EAAEC,uBAAuB,CAAC;QACjDa,aAAa,EAAE;UACXC,IAAI,EAAE,EAAE;UACRC,MAAM,EAAE,OAAO;UACfC,KAAK,EAAE;QACX,CAAC;QACDC,MAAM,EAAE,IAAI;QACZC,WAAW,EAAE,cAAc;QAC3BC,SAAS,EAAE,IAAI,CAACC,eAAe;QAC/BC,UAAU,EAAE,IAAI,CAACC,gBAAgB;QACjCC,MAAM,EAAElB,MAAM,CAACmB,MAAM,CAAC5B,KAAK,CAAC,CAAC6B,GAAG,CAACC,GAAE,IAAKrB,MAAM,CAACC,MAAM,CAAC;UAClDF,KAAK,EAAEsB,GAAG,CAACtB,KAAK;UAChBuB,IAAI,EAAED,GAAG,CAACE,UAAU,CAAC,CAAC,IAAI,GAAE,GAAIC,MAAM,CAACH,GAAG,CAACE,UAAU,CAAC,CAAC,CAAC,CAAC,CAACE,QAAQ,CAAC,CAAC,EAAE,GAAG,IAAI,GAAE,