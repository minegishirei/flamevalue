      \"{{ score.overtime }}\", \n                \"{{ score.count }}\",\n                \"{{ score.qiita_score }}\",\n                \"{{ score.remote }}\"\n            ],\n            // データライン\n            borderColor: '#1ABB9C',\n        }],\n    },\n    //オプションの設定\n    options: {\n        scales: {\n            r: {\n                //グラフの最小値・最大値\n                min: 0,\n                max: 5,\n                //背景色\n                backgroundColor: 'snow',\n                //グリッドライン\n                grid: {\n                    color: '#1ABB9C',\n                },\n                //アングルライン\n                angleLines: {\n                    color: '#1ABB9C',\n                },\n                //各項目のラベル\n                pointLabels: {\n                    color: '#1ABB9C',\n                },\n            },\n        },\n    },\n});\n\nexport default {\n    name: 'MyScores',\n    data() {\n        return {\n            basic : {\n                money: 100,\n                qiita_score: 100\n            }\n        }\n    },\n    importScript() {\n      const script = document.createElement('script')\n      script.setAttribute('src', 'https://www.youtube.com/iframe_api')\n      script.setAttribute('id', 'script_id')\n      document.head.appendChild(script)\n    }\n}\n</script>\n\n\n<style>\n.scores_size_controller {\n    max-width: 300px;\n}\n</style>\n\n"],"mappings":"AA2FA,OAAO,kDAAkD;AACzD,OAAO,gEAAgE;AAGvE,IAAIA,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC;AACjD,IAAIC,YAAW,GAAI,IAAIC,KAAK,CAACJ,GAAG,EAAE;EAC9B;EACAK,IAAI,EAAE,OAAO;EACb;EACAC,IAAI,EAAE;IACFC,MAAM,EAAE,CAAC,MAAM,EAAE,MAAM,EAAE,KAAK,EAAE,OAAO,EAAC,OAAO,CAAC;IAChDC,QAAQ,EAAE,CAAC;MACPC,KAAK,EAAE,YAAY;MACnB;MACAH,IAAI,EAAE,CACF,mBAAmB,EACnB,sBAAsB,EACtB,mBAAmB,EACnB,yBAAyB,EACzB,oBAAmB,CACtB;MACD;MACAI,WAAW,EAAE;IACjB,CAAC;EACL,CAAC;EACD;EACAC,OAAO,EAAE;IACLC,MAAM,EAAE;MACJC,CAAC,EAAE;QACC;QACAC,GAAG,EAAE,CAAC;QACNC,GAAG,EAAE,CAAC;QACN;QACAC,eAAe,EAAE,MAAM;QACvB;QACAC,IAAI,EAAE;UACFC,KAAK,EA