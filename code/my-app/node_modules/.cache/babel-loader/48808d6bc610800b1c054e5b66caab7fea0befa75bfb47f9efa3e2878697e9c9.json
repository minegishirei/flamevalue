\n        </div>\r\n    </div>\r\n\r\n    <!-- Account Page -->\r\n    <div class=\"account-page\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"col-2\">\r\n                    <img src=\"images/image1.png\" width=\"100%\">\r\n                </div>\r\n                <div class=\"col-2\">\r\n                    <div class=\"form-container\">\r\n                        <div class=\"form-btn\">\r\n                            <span onclick=\"login()\">Login</span>\r\n                            <span onclick=\"register()\">Register</span>\r\n                            <hr id=\"Indicator\">\r\n                        </div>\r\n                        <form id=\"LoginForm\">\r\n                            <input type=\"text\" placeholder=\"Username\">\r\n                            <input type=\"password\" placeholder=\"Password\">\r\n                            <button type=\"submit\" class=\"btn\">Login</button>\r\n                            <a href=\"\">Forget Password</a>\r\n                        </form>\r\n\r\n                        <form id=\"RegForm\">\r\n                            <input type=\"text\" placeholder=\"Username\">\r\n                            <input type=\"email\" placeholder=\"Email\">\r\n                            <input type=\"password\" placeholder=\"Password\">\r\n                            <button type=\"submit\" class=\"btn\">Register</button>\r\n                        </form>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n    <!-- Footer -->\r\n    <div class=\"footer\">\r\n        <div class=\"container\">\r\n            <div class=\"row\">\r\n                <div class=\"footer-col-1\">\r\n                    <h3>Download Our App</h3>\r\n                    <p>Download App for Android and ios mobile phone.</p>\r\n                    <div class=\"app-logo\">\r\n                        <img src=\"images/play-store.png\">\r\n                        <img src=\"images/app-store.png\">\r\n                    </div>\r\n                </div>\r\n                <div class=\"footer-col-2\">\r\n                    <img src=\"images/logo-white.png\">\r\n                    <p>Our Purpose Is To Sustainably Make the Pleasure and Benefits of Sports Accessible to the Many.\r\n                    </p>\r\n                </div>\r\n                <div class=\"footer-col-3\">\r\n                    <h3>Useful Links</h3>\r\n                    <ul>\r\n                        <li>Coupons</li>\r\n                        <li>Blog Post</li>\r\n                        <li>Return Policy</li>\r\n                        <li>Join Affiliate</li>\r\n                    </ul>\r\n                </div>\r\n                <div class=\"footer-col-4\">\r\n                    <h3>Follow Us</h3>\r\n                    <ul>\r\n                        <li>Facebook</li>\r\n                        <li>Twitter</li>\r\n                        <li>Instagram</li>\r\n                        <li>Youtube</li>\r\n                    </ul>\r\n                </div>\r\n            </div>\r\n            <hr>\r\n            <p class=\"copyright\">Copyright 2020 - Samwit Adhikary</p>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- javascript -->\r\n\r\n\r\n    <component :is=\"'script'\">\r\n        var MenuItems = document.getElementById(\"MenuItems\");\r\n        MenuItems.style.maxHeight = \"0px\";\r\n        function menutoggle() {\r\n            if (MenuItems.style.maxHeight == \"0px\") {\r\n                MenuItems.style.maxHeight = \"200px\"\r\n            }\r\n            else {\r\n                MenuItems.style.maxHeight = \"0px\"\r\n            }\r\n        }\r\n    </component>\r\n\r\n    <!-- Toggle Form -->\r\n\r\n    <component :is=\"'script'\">\r\n        var LoginForm = document.getElementById(\"LoginForm\");\r\n        var RegForm = document.getElementById(\"RegForm\");\r\n        var Indicator = document.getElementById(\"Indicator\");\r\n        function register() {\r\n            RegForm.style.transform = \"translatex(0px)\";\r\n            LoginForm.style.transform = \"translatex(0px)\";\r\n            Indicator.style.transform = \"translateX(100px)\";\r\n\r\n        }\r\n        function login() {\r\n            RegForm.style.transform = \"translatex(300px)\";\r\n            LoginForm.style.transform = \"translatex(300px)\";\r\n            Indicator.style.transform = \"translate(0px)\";\r\n\r\n        }\r\n        </component>\r\n\r\n\r\n</body>\r\n\r\n</html>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\n\r\nimport store from \"../store/store\";\r\nexport default {\r\n    name: 'WakouAcount',\r\n    data() {\r\n        document.title = \"和光イベントカレンダー ~和光市で開催されるイベント一覧~\"\r\n        return {\r\n            store_top3: Object.fromEntries(Object.entries(store).slice(0, 3)),\r\n            store_after3: Object.fromEntries(Object.entries(store).slice(3,)),\r\n            store: store\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AA4JA,OAAOA,KAAI,MAAO,gBAAgB;AAClC,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACHC,QAAQ,CAACC,KAAI,GAAI,+BAA8B;IAC/C,OAAO;MACHC,UAAU,EAAEC,MAAM,CAACC,WAAW,CAACD,MAAM,CAACE,OAAO,CAACR,KAAK,CAAC,CAACS,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;MACjEC,YAAY,EAAEJ,MAAM,CAACC,WAAW,CAACD,MAAM,CAACE,OAAO,CAACR,KAAK,CAAC,CAACS,KAAK,CAAC,CAAE,CAAC,CAAC;MACjET,KAAK,EAAEA;IACX;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}